#lang racket/base
(define(compare-adjacent n)
  (define(iter n first last count )
    
      (if(= n 0)
         
         count
         
      
       (if(> last first)
            
          (iter (quotient n 10) (remainder n 10)(remainder(quotient n 10)10) (+ count 1))
          (iter (quotient n 10) (remainder n 10)(remainder(quotient n 10)10) count))))
          
     (iter (quotient n 10) (remainder n 10)(remainder(quotient n 10)10) 0))
(compare-adjacent 52741 )
